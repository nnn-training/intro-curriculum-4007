extends layout

block content
  h1= title
  p Welcome to #{title}
  if user
    p Hello, #{user.username}
    a(href="/logout") Logout
  else
    a(href="/login") Login
  #scaling-block.block
  button#scaling-button 拡大縮小
  #moving-block.block
  button#moving-button 右に移動
  script(src="/javascripts/bundle.js")