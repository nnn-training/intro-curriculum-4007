extends layout

block content
  h1= title
  p Welcome to #{title}
  if user
    p Hello, #{user.username}
    a(href="/logout") Logout
  else
    a(href="/login") Login
  div#block.block
  p
    button#scaling-button 大きくなって小さくなるよ♪
  p
    button#moving-button スライドするよ♪
  script(src="/javascripts/bundle.js")